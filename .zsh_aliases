# Jay Shepherd's meager aliases
# ----------------------------------------------------------
alias rs='bundle exec rails server'
alias rc='bundle exec rails console'
alias cuke='bundle exec cucumber --require=features'
alias ct='bundle exec cucumber --require=features --tag'
alias rsr='touch tmp/restart.txt'
alias bi='bundle install'
alias biv='bundle install --path=vendor/bundle'
alias rgtandem='rvm gemset delete potsdam'

# mySql aliases
alias mysql='/usr/local/mysql/bin/mysql'
alias mysqladmini='/usr/local/mysql/bin/mysqladmin'

# Misc asliases
alias szsh='source ~/.zshrc'
